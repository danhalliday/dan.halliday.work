.d-background-graphic-light {
  background-image: url("<%= svg_data_uri("d-background-graphic-white.min.svg") %>");
  background-size: 125% auto;
}

.d-background-graphic-dark {
  background-image: url("<%= svg_data_uri("d-background-graphic-black.min.svg") %>");
  background-size: 125% auto;
}

.d-body {
  @extend .d-background-graphic-light;
}

.d-footer {
  @extend .d-background-graphic-dark;
}
